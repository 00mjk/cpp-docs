{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "group-vs",
        "src": ".",
        "exclude": [
          "**/obj/**",
          "vcppdocs/**",
          "**/includes/**",
          "vs-2015/**"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "group-vs2015",
        "src": "./vs-2015",
        "exclude": [
          "**/obj/**",
          "vcppdocs/**",
          "**/includes/**"
        ]
      }
        ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "group": "group-vs",
        "src": ".",
        "exclude": [
          "**/obj/**",
          "vcppdocs/**",
          "**/includes/**",
          "vs-2015/**"
        ]
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "group": "group-vs2015",
        "src": "vs-2015",
        "exclude": [
          "**/obj/**",
          "vcppdocs/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
            "breadcrumb_path": "~/_breadcrumb/toc.yml",
            "ROBOTS": "INDEX,FOLLOW",
            "manager": "wpickett",
            "ms.date": "11/16/2016",
            "ms.topic": "conceptual",
            "audience": "developer",
            "ms.prod": "visual-cpp",
            "ms.tgt_pltfrm": "",
            "searchScope": ["C++"],
            "feedback_system": "GitHub",
            "feedback_github_repo": "MicrosoftDocs/cpp-docs",
            "feedback_product_url": "https://developercommunity.visualstudio.com/spaces/62/index.html"
    },
    "fileMetadata": {
      "breadcrumb_path": 
      {
        "**/*.md": "_breadcrumb/toc.json",
        "vs-2015/**/*.md": "vs-2015/_breadcrumb/toc.json"
      },
      "monikerRange": {

        "mfc/**/*.md": "= vs-2015 || >= vs-2017"
    }
    },


    "template": [],
    "xref": ["msdn.4.5.2.zip", "namespaces.4.5.2.zip", "vs.110.zip", "vs.140.zip", "office15.zip"],
    "dest": "vcppdocs",
    "groups": {
      "group-vs": {
        "moniker_range": ">= vs-2017",
        "dest": "vcppdocs-2017"
      },
      "group-vs2015": {
        "moniker_range": "vs-2015",
        "dest": "vcppdocs-2015"
      }
    }
  }
}